{
  "name": "sqltools-singlestore-driver",
  "displayName": "SQLTools SingleStore Driver",
  "description": "SQLTools Driver for SingleStore",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "publisher": "singlestore",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/memsql/sqltools-singlestore-driver"
  },
  "bugs": "https://github.com/memsql/sqltools-singlestore-driver/issues",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test:tsc": "tsc --noEmit -p ./tsconfig.json --skipLibCheck"
  },
  "keywords": [
    "sqltools-driver",
    "vs-code",
    "singlestore",
    "singlestoredb",
    "memsql",
    "sql"
  ],
  "homepage": "https://github.com/memsql/sqltools-singlestore-driver#readme",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "extensionDependencies": [
    "mtxr.sqltools"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "dependencies": {
    "@sqltools/base-driver": "latest",
    "@sqltools/types": "latest",
    "lodash": "^4.17.19",
    "mysql": "^2.18.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/jest": "^29.4.0",
    "@types/node": "^14.0.9",
    "@types/vscode": "^1.72.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typescript": "~4.8.3"
  }
}
