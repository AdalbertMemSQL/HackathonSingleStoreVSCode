{
  "name": "sqltools-driver-singlestore",
  "displayName": "SingleStore",
  "description": "SQLTools Driver for SingleStore",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "publisher": "mtxr",
  "license": "MIT",
  "repository": {},
  "bugs": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test:tsc": "tsc --noEmit -p ./tsconfig.json --skipLibCheck"
  },
  "keywords": [
    "driver-template",
    "sqltools-driver"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "extensionDependencies": [
    "mtxr.sqltools"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "dependencies": {
    "@mysql/xdevapi": "^8.0.20",
    "@sqltools/base-driver": "latest",
    "@sqltools/formatter": "latest",
    "@sqltools/log": "latest",
    "@sqltools/types": "latest",
    "lodash": "^4.17.19",
    "mysql": "^2.18.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/jest": "^29.4.0",
    "@types/node": "^14.0.9",
    "@types/vscode": "^1.72.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typescript": "~4.8.3"
  }
}
